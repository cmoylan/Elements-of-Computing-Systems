/**
 * Or gate: out = 1 if {a==1 or b==1}, 0 otherwise
 */

CHIP Or {

    IN a, b;
    OUT out;

    PARTS:
    // Put your code here.
    // Or = Nand(Not(a), Not(b))

    Not(in=a, out=notA);
    Not(in=b, out=notB);

    Nand(a=notA, b=notB, out=out);

}
